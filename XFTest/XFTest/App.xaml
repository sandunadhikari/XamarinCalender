            <Style x:Key="CalDateText" TargetType="Label">
                <Setter Property="TextColor" Value="{StaticResource ColorBluishGrey}" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="VisualStateManager.VisualStateGroups">
                    <VisualStateGroupList>
                        <VisualStateGroup x:Name="SizeStates">

                            <VisualState x:Name="Small">
                                <VisualState.Setters>
                                    <Setter Property="FontSize" Value="{OnPlatform iOS=14, Android=11}"/>
                                </VisualState.Setters>
                            </VisualState>

                            <VisualState x:Name="Medium">
                                <VisualState.Setters>
                                    <Setter Property="FontSize" Value="{OnPlatform iOS=16, Android=12}"/>
                                </VisualState.Setters>
                            </VisualState>

