            <Style
                x:Key="buttonStyleList"
                TargetType="Button">
                <Setter
                    Property="WidthRequest"
                    Value="100" />
                <Setter
                    Property="HeightRequest"
                    Value="30" />
                <Setter
                    Property="TextColor"
                    Value="White" />
                <Setter
                    Property="FontAttributes"
                    Value="Bold" />
                <Setter
                    Property="VisualStateManager.VisualStateGroups">
                    <VisualStateGroupList>
                        <VisualStateGroup
                            x:Name="SizeStates">

                            <VisualState
                                x:Name="Small">
                                <VisualState.Setters>
                                    <Setter
                                        Property="FontSize"
                                        Value="{OnPlatform iOS=12, Android=11}" />
                                </VisualState.Setters>
                            </VisualState>

                            <VisualState
                                x:Name="Medium">
                                <VisualState.Setters>
                                    <Setter
                                        Property="FontSize"
                                        Value="{OnPlatform iOS=14, Android=13}" />
                                </VisualState.Setters>
                            </VisualState>

                            <VisualState
                                x:Name="Large">
                                <VisualState.Setters>
                                    <Setter
                                        Property="FontSize"
                                        Value="{OnPlatform iOS=16, Android=14}" />
                                </VisualState.Setters>
                            </VisualState>

                        </VisualStateGroup>
                    </VisualStateGroupList>
                </Setter>
            </Style>

