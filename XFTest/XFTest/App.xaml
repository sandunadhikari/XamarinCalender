            <OnPlatform
                x:TypeArguments="x:String"
                x:Key="BoldFont">
                <On
                    Platform="iOS"
                    Value="OpenSans-Bold" />
                <On
                    Platform="Android"
                    Value="OpenSans-Bold.ttf#Bold" />
            </OnPlatform>

            <OnPlatform
                x:TypeArguments="x:String"
                x:Key="RegularFont">
                <On
                    Platform="iOS"
                    Value="OpenSans-Regular" />
                <On
                    Platform="Android"
